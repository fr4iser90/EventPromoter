{
  "name": "@eventpromoter/types",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./src/schema/index.ts",
      "default": "./dist/schema/index.js"
    },
    "./schema/*": {
      "types": "./src/schema/*.ts",
      "default": "./dist/schema/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "echo \"No lint configured for @eventpromoter/types\"",
    "test": "echo \"No tests for @eventpromoter/types\"",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}
